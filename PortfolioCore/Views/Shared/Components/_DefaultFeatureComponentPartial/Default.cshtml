@model List<SocialMedia>
<section id="hero" class="hero section light-background">

    <img src="/MyResume-1.0.0/assets/img/6280989.jpg" alt="">

    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">
            <div class="col-lg-9">
                <h2>Kağan Çevik</h2>
                <p>Çalışma Alanlarım: <span class="typed" data-typed-items="ASP.NETCORE">Designer</span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
                <div class="social-links">
                    @foreach (var item in Model)
                    {
                    <a href="@item.Url"><i class="@item.Link"></i></a>
                    }
                </div>
            </div>
        </div>
    </div>

</section><!-- /Hero Section -->